{
  "name": "<%= name.toLowerCase() %>",
  "description": "<%= description %>",
  "version": "0.0.0",
  "author": "<%= author %>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "develop": "gatsby develop -H 0.0.0.0 -p 3000",
    "develop:docs": "NODE_ENV=test start-storybook -p 3002 -s ./static --ci --quiet --docs",
    "build": "gatsby build",
    "build:docs": "NODE_ENV=test build-storybook -o ./docs -s ./static --quiet --docs",
    "generate": "hygen new",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@emotion/core": "^10.0.0",
    "@svgr/webpack": "^4.1.0",
    "gatsby": "^2.3.0",
    "gatsby-image": "^2.0.0",
    "gatsby-plugin-emotion": "^4.0.0",
    "gatsby-plugin-graphql-codegen": "^2.7.0",
    "gatsby-plugin-layout": "^1.0.0",
    "gatsby-plugin-manifest": "^2.0.0",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-next-seo": "^1.5.0",
    "gatsby-plugin-notify": "^1.0.0",
    "gatsby-plugin-nprogress": "^2.0.0",
    "gatsby-plugin-offline": "^2.0.0",
    "gatsby-plugin-polyfill-io": "^1.0.0",
    "gatsby-plugin-postcss": "^2.0.0",
    "gatsby-plugin-preconnect": "^1.0.0",
    "gatsby-plugin-react-helmet-async": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sitemap": "^2.0.0",
    "gatsby-plugin-sri": "^1.1.0",
    "gatsby-plugin-svgr": "^2.0.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.0",
<% if (source === 'prismic') { -%>
    "gatsby-source-prismic": "^3.0.0",
    "react-html-renderer": "^0.3.0",
    "dayjs": "^1.8.0",
<% } -%>
    "medium-zoom": "^1.0.0",
    "pollen-css": "^0.5.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-helmet-async": "^1.0.0",
<% if (components.includes('adaptiveCarousel')) { -%>
    "react-intersection-observer": "^8.26.0",
    "react-spring": "^8.0.0",
<% } -%>
<% if (components.includes('form')) { -%>
    "react-textarea-autosize": "^7.1.0",
    "react-range": "^1.1.0",
    "react-select": "^3.0.4",
<% } -%>
<% if (components.includes('toasts')) { -%>
    "react-toast-notifications": "^2.4.0",
<% } -%>
<% if (components.includes('video')) { -%>
    "react-player": "^1.12.0",
<% } -%>
<% if (components.includes('modal')) { -%>
    "react-modal": "^3.11.0",
<% } -%>
    "react-responsive": "^8.0.0",
    "react-use": "^10.1.0",
    "satchel-css": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@emotion/babel-preset-css-prop": "^10.0.0",
    "@storybook/addon-docs": "^5.3.0",
    "@storybook/addon-essentials": "^5.3.0",
    "@storybook/addons": "^5.3.0",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-headroom": "^2.2.0",
<% if (components.includes('modal')) { -%>
    "@types/react-modal": "^3.10.0",
<% } -%>
    "@types/react-responsive": "^8.0.2",
<% if (components.includes('toasts')) { -%>
    "@types/react-toast-notifications": "^2.4.0",
<% } -%>
<% if (components.includes('form')) { -%>
    "@types/react-select": "^3.0.0",
    "@types/react-textarea-autosize": "^4.3.0",
<% } -%>
    "babel-loader": "^8.0.0",
    "babel-plugin-remove-graphql-queries": "^2.6.0",
    "babel-preset-gatsby": "^0.2.0",
    "core-js": "^3.6.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "hygen": "^5.0.0",
    "prettier": "^1.18.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.0"
  }
}
